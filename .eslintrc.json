{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "max-len": ["warn", {"code": 120}],
        "indent": ["error", 4],
        "valid-jsdoc": "off"
    },
    "ignorePatterns": "built/"
}
